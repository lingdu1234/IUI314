{
  "extends": "@tsconfig/node18/tsconfig.json",
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "./vite/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "types": [
      "node",
      "vite-plugin-svg-icons/client",
      "vite/client",
      "unplugin-vue-define-options/macros-global",
      "element-plus/global"
    ]
  }
}